{
  "name": "@lls/diskusage",
  "private": false,
  "version": "1.2.0",
  "description": "Get total diskspace and free diskspace using bindings around platform specific calls.",
  "main": "index.js",
  "typings": "index.d.ts",
  "gypfile": true,
  "dependencies": {
    "es6-promise": "^4.2.5",
    "node-addon-api": "^2.0.0",
    "prebuild-install": "6.1.4"
  },
  "devDependencies": {
    "eslint": "7.4.0",
    "node-abi": "3.5.0",
    "node-gyp": "7.0.0",
    "prebuild": "10.0.1"
  },
  "scripts": {
    "install": "prebuild-install || node-gyp rebuild",
    "prebuild-node": "prebuild -t 16.9.1 --strip",
    "prebuild-node-ia32": "prebuild -t 16.9.1 -a ia32 --strip",
    "prebuild-electron": "prebuild -t 16.0.2 -r electron --strip",
    "prebuild-electron-ia32": "prebuild -t 16.0.2 -r electron -a ia32 --strip"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lelivrescolaire/node-diskusage.git"
  },
  "keywords": [
    "disk",
    "usage",
    "df",
    "hdd",
    "ssd",
    "diskusage",
    "df",
    "free",
    "space"
  ],
  "author": "Maximilien DERRIER <max.derrier@lelivrescolaire.fr>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lelivrescolaire/node-diskusage/issues"
  },
  "homepage": "https://github.com/lelivrescolaire/node-diskusage"
}
